{
  "name": "xo",
  "version": "1.1.1",
  "description": "A 1v1 round-based browser-game. It uses Nuxt.js for app structure and Socket.io for real-time. The client is based on Vuetify.",
  "author": {
    "name": "Daniel Funiok",
    "email": "dafuniok@gmail.com"
  },
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "dev": "nodemon -w sockets -w server.js -w nuxt.config.js server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "generate": "nuxt generate",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/vuetify": "^1.9.1",
    "cross-env": "^7.0.2",
    "express": "^4.14.0",
    "nuxt": "^2.11.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "vue-i18n": "^8.17.2"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^24.1.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "vue-jest": "^4.0.0-0"
  }
}
